<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d %-5level %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="com.socks" level="INFO" addivity="false">
        <appendet-ref ref="STDOUT"/>
    </logger>

    <root level="debug">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>